<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bg.web.menu.menuMapper">
    <select id="getCateList" resultType="com.bg.web.model.MenuVO">
        <![CDATA[
            SELECT CATE_ID
                 , CATE_NM
                 , CATE_DEPTH
                 , REG_ID
                 , REG_DTE
                 , EDIT_ID
                 , EDIT_DTE
                 , SORT
              FROM BOCATEMT;
          ]]>
    </select>
</mapper>